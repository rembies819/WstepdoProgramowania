print('Dodanie %(liczba1)d do %(liczba2)d to %(wynik)d' % {'liczba1': 3, 'liczba2': 4, 'wynik': 7})
print('Dodanie {0} do {1} to {2}'.format(3, 4, 7))
liczba1, liczba2 = 3, 4
print('Dodanie {1} do {0} to {2}'.format(liczba1, liczba2, liczba1 + liczba2))
print(type(liczba1))
print(type(ala))
