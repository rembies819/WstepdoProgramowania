a = 8
b = 4
c = 3
dzielenie = a/b
print('Dzielenie to: ', dzielenie)
dzielenie = a/c
print('Dzielenie to: ', dzielenie)
dodawanie = a+b
print('Dodawanie to: ', dodawanie)
dzielenie_calkowite = a//c
print('Dzielenie calkowite to: ', dzielenie_calkowite)
reszta = a % c
print('Dzielenie z reszta to: ', reszta)
potega = b ** c
print('Potega to: ', potega)
potega = pow(4, 3)
print('Potega to', potega)

d = pow(math.e, 10)
print(d)
e = pow((math.log(5 + math.exp(math.sin(8)))), 1/6)
print(e)
print(math.ceil(4.80))
print(math.floor(3.55))
print("światło")
print('jaźń')
